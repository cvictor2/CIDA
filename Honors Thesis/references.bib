@TechReport{LT15,
  Title                    = {Partitioned Second Order Method for Magnetohydrodynamics in {Els{\"a}sser} variables},
  Author                   = {Li, Y. and Trenchea, C.},
  Institution              = {{Pittsburgh University, Department of Mathematics}},
  Year                     = {February, 2015},

  Address                  = {Pittsburgh},
  Number                   = {TR-MATH 15-03},

  Abstract                 = {Magnetohydrodynamics (MHD) studies the dynamics of electrically conducting fluids, involving Navier-Stokes
equations coupled with Maxwell equations via Lorentz force and Ohm’s law. Monolithic methods, which solve fully coupled
MHD systems, are computationally expensive. Partitioned methods, on the other hand, decouple the full system and solve
subproblems in parallel, and thus reduce the computational cost.
This paper is devoted to the design and analysis of a partitioned method for the MHD system in the Els¨asser fields. We
perform the stability analysis and find that for magnetic Prandtl number of order unity, the method is reasonably stable. we
derive the complete error estimate and present a computational test to support the theory.},
  HowPublished             = {http://www.pitt.edu/~yol34/MHD.pdf}
}

